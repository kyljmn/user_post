<h3><%= post.title %></h3>
<p><%= "by: " + post.user.username %></p>
<p><%= post.text %></p>
<% if current_user.id == post.user.id %>
  <%= link_to "Show Post", post_path(post) %>
  <%= link_to "Edit Post", edit_post_path(post) %>
  <%= link_to "Delete Post", post_path(post), method: :delete %>
<% end %>